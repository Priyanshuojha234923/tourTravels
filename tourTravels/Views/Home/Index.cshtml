@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/GeneralMaster.cshtml";
}

@using tourTravels.Models;
@using System.Data;
@using System.Data.SqlClient;
<html>

<head>
    <link href="~/Content/css/index.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.7.1.min.js"></script>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/5.0.0-alpha1/js/bootstrap.min.js" />
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/5.0.0-alpha1/css/bootstrap.min.css" />
    <style>
        .card-body {
            height: 500px;
            width: 300px;
            margin-right: 5px;
        }
        ::-webkit-scrollbar {
            display: none;
        }
    </style>
</head>
<body>
    <section>
        <div id="carouselExampleInterval" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active" data-bs-interval="10000">
                    <img src="~/Content/image/pexels-frans-van-heerden-632522.jpg" class="d-block w-100" alt="..." style="max-height: 600px; opacity: 0.7">
                    <div class="carousel-caption ">
                        <span>London</span>
                        <p>Some representative placeholder content for the first slide.</p>
                    </div>
                </div>
                <div class="carousel-item" data-bs-interval="2000">
                    <img src="~/Content/image/pexels-elijah-odonnell-2409681.jpg" class="d-block w-100" alt="..." style="max-height: 600px; opacity: 0.7">
                    <div class="carousel-caption ">
                        <span>Paris</span>
                        <p>Some representative placeholder content for the first slide.</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img src="~/Content/image/pexels-elijah-odonnell-2409681.jpg" class="d-block w-100" alt="..." style="max-height:600px;opacity:0.7">
                    <div class="carousel-caption align-items-center ">
                        <span>austrelia</span>
                        <p>Some representative placeholder content for the first slide.</p>
                    </div>
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleInterval" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleInterval" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>

    </section>
    <senction style="background-color:red">
     

    </senction>


    <section style="background-color: #f5fffa ">
        <div class="container" style="background-color: #f5fffa ">

            <div class="row mb-5" style="background-color: #f5fffa; ">
                <div class="col-lg-8 mx-auto" style="margin-top:5%">

                    <div class=" p-5 rounded shadow" style="background-color: #f5fffa">

                        <form>
                            <div class="row mb-4">
                              
                                <select class="form-select form-select-lg mb-3" aria-label=".form-select-lg example" id="txtplace">
                                    <option>Where You Want to go</option>
                                </select>
                                <input type="button" value="book Now" class="btn btn-success" id="btnsave" data-bs-toggle="modal" data-bs-target="#exampleModal" />
                            </div>


                        </form>

                    </div>
                </div>
            </div>


        </div>
    </section>
  
    <div class="container-fluid bg-opacity-75" style="background-color: #bed5e0;border-radius:50px;">
        <center> <span style="font-size:45px">Our Top Places</span></center>
        <div class="grid text-center overflow-x-scroll" style="display:flex;justify-content:space-between;">

            @{
                DBManager db = new DBManager();
                string cmd = "select * from tbl_place";
                System.Data.DataTable dt = db.getallrecord(cmd);
                if (dt.Rows.Count > 0)
                {
                    for (int i = 0; i < dt.Rows.Count; i++)
                    {
                        <div class="card-body" style="height:400px" >
                        <img class="rounded-top-4"  src="~/content/image/@dt.Rows[i]["file"]" style="height:300px;width:300px;border-radius:20px"  />
                            <h2 class="card-subtitle">@dt.Rows[i]["place"]</h2>
                            <h5><del style="color:red">$@dt.Rows[i]["orate"]</del>   <i style="color:green">$@dt.Rows[i]["nrate"]</i></h5>
                            <a href="/home/index" style="text-decoration:none">Reed More</a>
                        </div>
                    }
                }
            }


        </div>

    </div>



  

